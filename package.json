{
  "name": "@zhengxy/use",
  "version": "0.0.6-beta.2",
  "description": "Composition API",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "node ./build.js -w",
    "build": "pnpm clear && pnpm build:bundle && pnpm build:types",
    "clear": "rimraf ./dist",
    "build:types": "tsc -p ./tsconfig.json",
    "build:bundle": "node ./build.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.browser.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikey-labs/use.git"
  },
  "keywords": [
    "utils",
    "api",
    "tools"
  ],
  "author": "mikey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikey-labs/use/issues"
  },
  "homepage": "https://github.com/mikey-labs/use#readme",
  "devDependencies": {
    "esbuild": "^0.17.7",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
